<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
       "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
  
    <definition name="defaultTemplate" template="/WEB-INF/views/template/template.jsp">
        <put-attribute name="header" value="/WEB-INF/views/template/header.jsp" cascade="true"/>
        <put-attribute name="menu" value="/WEB-INF/views/template/menu.jsp" cascade="true"/>
        <put-attribute name="footer" value="/WEB-INF/views/template/footer.jsp" cascade="true"/>
        <put-attribute name="body" value="/WEB-INF/views/ARP/home.jsp" cascade="true"/>
    </definition>
    
    <definition name="homePage" extends="defaultTemplate">
		<put-attribute name="body" value="/ARP/home.jsp" />
    </definition>
  
</tiles-definitions>